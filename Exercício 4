#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
 int num;

 printf("digite um numero entre 10 e 100:\n");
 scanf("%d", &num);
 printf("\n");
 while(num >= 10 && num <= 100)
    {

      if(num % 10 == 0)
      {
        printf("Numero terminado em zero \n");
      }
      else
      {
       printf("O numero e:%d\n", num);
      }

 printf("digite um numero entre 10 e 100:\n");
 scanf("%d", &num);
    }

printf("Saindo...");

}

